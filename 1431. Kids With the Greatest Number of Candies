class Solution {
    public List<Boolean> kidsWithCandies(int[] candies, int extraCandies) {

        // Initialize maximum element 
         int max = candies[0]; 

         // Traverse array elem to find the highest number  
         for (int i = 1; i < candies.length; i++) 
             if (candies[i] > max) 
                 max = candies[i]; 
